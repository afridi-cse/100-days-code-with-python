   "source": [
    "from datetime import date, timedelta\n",
    "import calendar\n",
    "start_date = date(2022, 12, 25)\n",
    "days_in_month = calendar.monthrange(start_date.year, start_date.month)[1]\n",
    "print(start_date + timedelta(days=days_in_month))\n"
   ]
  }
